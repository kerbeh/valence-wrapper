{
    "name": "kerbeh/valence-wrapper",
    "description": "Wrapper for the Brightspace Valence API SDK",
    "type": "library",
    "authors": [
        {
            "name": "Tristan Kirby",
            "email": "t.kirby@deakin.edu.au"
        }
    ],
    "repositories": [
        {
            "type": "git",
            "url": "https://github.com/Brightspace/valence-sdk-php"
        }
    ],
    "minimum-stability": "stable",
    "require": {
        "php": ">=5.6.0",
        "brightspace/valence-sdk-php": "~1.6",
        "guzzlehttp/guzzle": "^6.3"
    },
    "require-dev": {
        "phpunit/phpunit": "^7.0",
        "codedungeon/phpunit-result-printer": "^0.23.4"
    },
    "scripts": {
        "test": "phpunit"
    },
    "autoload": {
        "psr-4": {
            "ValenceWrapper\\": "src/"
        }
    }
}
